<h1>Update Account Details</h1>
<%= render(partial: 'error_message', collection: @user.errors.full_messages) if !@user.errors.empty?%>
<%= form_for @user do |f| %>
  <div>
    <%= f.label :email, "Email" %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :nickname, "Nickname" %>
    <%= f.text_field :nickname %>
  </div>
  <div>
    <%= f.submit "Update" %> <%= link_to "Cancel", :back %>
  </div>
<% end %>
